    <TextView
            android:id="@+id/link4"
            android:layout_width="165sp"
            android:layout_height="50sp"
            android:textColor="@android:color/background_light"
            android:fontFamily="@font/exo_2"
            android:text="@string/link4"
            android:textSize="14sp"
            android:clickable="true"
            android:translationX="190sp"
            android:translationY="450sp"
            android:visibility="visible" android:textColorLink="@color/colorPrimary"/>

    <TextView
            android:id="@+id/link5"
            android:layout_width="165sp"
            android:layout_height="50sp"
            android:textColor="@android:color/background_light"
            android:fontFamily="@font/exo_2"
