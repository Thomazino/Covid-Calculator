<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/darkbackground"
                tools:context=".Probabilities">

    <Button
            android:id="@+id/button12"
            android:background="@drawable/buttonbackground"
            android:layout_centerHorizontal="true"
            android:layout_width="73dp"
            android:text="Home"
            android:fontFamily="@font/exo_2"
            android:layout_below="@+id/textView3" android:layout_height="30dp" android:layout_marginTop="10dp"/>

    <TextView
            android:text="TextView"
            android:layout_width="310dp"
            android:layout_height="60dp" android:id="@+id/textView"
            android:layout_alignParentTop="true" android:layout_marginTop="18dp"
            android:textColorHighlight="@android:color/holo_blue_bright" android:visibility="visible"
            android:textColorHint="@android:color/background_light" android:textColor="@android:color/background_light"
            android:textSize="19sp" android:layout_centerHorizontal="true" android:textAlignment="center"
            android:textStyle="normal|bold|italic" android:fontFamily="@font/exo_2" android:autoSizeTextType="uniform"/>
    <TextView
            android:text="TextView"
            android:layout_width="333dp"
            android:layout_height="96dp" android:id="@+id/textView2"
            android:layout_centerHorizontal="true"
            android:textSize="19sp"
            android:visibility="visible" android:textStyle="normal|bold"
            android:textColor="#f28ae4f1"
            android:textAlignment="center"
            android:background="@android:drawable/alert_dark_frame"
            android:textColorHighlight="@android:color/background_light"
            android:textColorHint="@android:color/background_light"
            android:fontFamily="@font/exo_2" android:layout_below="@+id/textView"/>
    <TextView
            android:text="TextView"
            android:layout_width="309dp"
            android:layout_height="406dp" android:id="@+id/textView3"
            android:textColor="@android:color/background_light" android:textSize="16sp" android:visibility="visible"
            android:textAlignment="center"
            android:layout_centerHorizontal="true"
            android:textStyle="normal|bold"
            android:fontFamily="@font/exo_2"
            android:lineSpacingExtra="-3sp" android:layout_below="@+id/textView2" android:autoSizeTextType="uniform"
            android:layout_above="@+id/textView7"/>
    <TextView
            android:text="  "
            android:layout_width="wrap_content"
            android:layout_height="41dp" android:layout_alignParentEnd="false"
            android:layout_alignParentBottom="true" android:id="@+id/textView7" android:layout_centerHorizontal="true"
            android:layout_marginBottom="18dp"/>


</RelativeLayout>